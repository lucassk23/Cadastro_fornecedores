{
  "name": "cadastro_fornecedores",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^2",
    "@sap/cds": "^9",
    "@sap/cds-common-content": "^3.0.1",
    "express": "^4",
    "@sap/xssec": "^4"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.13.0",
    "@cap-js/sqlite": "^2",
    "@sap/cds-dk": ">=8",
    "mbt": "^1.2.29",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-cadastro_fornecedores_ui": "cds watch --open cadastro_fornecedores_ui/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy cadastro_fornecedores --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "cds": {
    "requires": {
      "[production]": {
        "auth": "mocked",
        "db": "hana"
      },
      "server": {
        "index": true
      }
    }
  },
  "sapux": [
    "app/cadastro_fornecedores_ui"
  ]
}
